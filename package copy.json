{
  "name": "cdktstempalate",
  "version": "0.1.0",
  "bin": {
    "zebrachallenge": "bin/cdk-ts-template.js"
  },
  "scripts": {
    "build": "tsc -noEmit true && npm run lint && npm run test",
    "build-only": "tsc -noEmit true",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "deploy-dev": "cdk deploy --profile <your-dev-profile>",
    "deploy-prod": "cdk deploy --profile <your-prod-profile>"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "aws-cdk": "2.95.1",
    "esbuild": "^0.19.7",
    "eslint": "^8.54.0",
    "jest": "^29.6.4",
    "prettier": "3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.95.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
